;;;;;;;;;;;;;;;;;;
; modules3rd.ini ;
;;;;;;;;;;;;;;;;;;

<% @nginx_modules3rds.each do |m| %>
[<%=m[:name]%>]
form=<%=m[:form]%>
url=<%=m[:url]%>
rev=<%=m[:rev] || 'master'%>
<% if m[:shprov] -%>
shprov=<%=m[:shprov]%>
<% end -%>
<% if m[:shprovdir] -%>
shprovdir=<%=m[:shprovdir]%>
<% end -%>
<% end -%>
